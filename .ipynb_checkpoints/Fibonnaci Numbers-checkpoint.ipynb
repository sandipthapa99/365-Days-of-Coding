    "def fibonnaci(n):\n",
    "    x = 0\n",
    "    y = 1\n",
    "    list = [x, y]\n",
    "    \n",
    "    while len(list) < n:\n",
    "        x, y = y , x+y\n",
    "        list.append(y)        \n",
    "    return list\n",
    "        \n",
    "fibonnaci(11)"
   ]
