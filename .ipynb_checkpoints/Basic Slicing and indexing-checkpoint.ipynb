    "import numpy as np\n",
    "\n",
    "a = np.arange(10)\n",
    "print(\"Original Array: \",a)\n",
    "\n",
    "print(\"\\na[2:8:2] = \",a[2:8:2]) # [start:stop:step]\n",
    "\n",
    "print(\"\\na[::] = \",a[::]) # from start to end\n",
    "\n",
    "print(\"\\na[::-1] = \",a[::-1]) # from end to start\n",
    "\n",
    "print(\"\\na[1:1] = \",a[1:1]) # between same index"
   ]
  }
