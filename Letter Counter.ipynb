    "word = input(\"Word: \")\n",
    "def fcount(word):\n",
    "    dict = {}\n",
    "    for letter in word:\n",
    "        if letter in dict:\n",
    "            dict[letter] += 1\n",
    "        else:\n",
    "            dict[letter] =1\n",
    "    for key,value in dict.items():\n",
    "        print(\"%s : %d times\"%(key,value))\n",
    "\n",
    "fcount(word)"
   ]
